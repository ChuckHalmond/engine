    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/web/dist/*.js",
            ],
            "runtimeExecutable": "C:/Users/Charles/AppData/Local/Chromium/Application/chrome.exe",
            "url": "http://localhost:5500",
            "webRoot": "${workspaceFolder}/web"
        }
    ]
}